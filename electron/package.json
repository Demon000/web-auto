{
  "name": "@web-auto/electron",
  "version": "1.0.0",
  "author": "Demon000",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "tsc && mv ./dist/esm/web-preload.js ./dist/esm/web-preload.mjs",
    "clean": "tsc --build --clean",
    "lint": "eslint --fix .",
    "start": "electron ./dist/esm/main.js",
    "start-debug": "electron --inspect-brk=5858 ./dist/esm/main.js",
    "prepare": "electron-rebuild -o bluetooth-socket && electron-rebuild -o usocket"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "async-mutex": "^0.4.0",
    "audify": "^1.8.2",
    "bluetooth-socket": "github:Demon000/node-bluetooth-socket#master",
    "bluez": "github:Demon000/node-bluez#master",
    "import-meta-resolve": "^4.0.0",
    "json5": "^2.2.3",
    "lilconfig": "^3.0.0",
    "native-duplexpair": "^1.0.0",
    "tinyh264": "^0.0.7",
    "usb": "^2.11.0",
    "usocket": "github:Demon000/usocket#master"
  },
  "overrides": {
    "usocket": "github:Demon000/usocket#master"
  }
}
